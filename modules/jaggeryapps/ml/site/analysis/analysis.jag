        </div>
        <div class="row">
            <div class="col-md-12">

                <!-- content -->
                <div class="container col-md-12 col-centered wr-content">

                    <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true"></div>

                </div>
                <!-- /content -->


            </div>
