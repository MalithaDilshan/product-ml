    <p>&copy; 2014 WSO2 Inc. All Rights Reserved</p>
</footer-->

<div id="content-asset-types" style="display: none">
    <div>
        <a class="ast-type-item" href="../project/projects.jag"><img src="../../images/icons/ico-projects.png" /><span>Projects</span></a>
        <a class="ast-type-item" href="../data/datasets.jag"><img src="../../images/icons/ico-datasets.png" /><span>Datasets</span></a>   
    </div>
</div> 

<script src="https://code.jquery.com/jquery-latest.min.js"></script>
<script src="../../js/wso2.ml.util.js"></script>
<script src="../../js/bootstrap.min.js"></script>

<script type="text/javascript">

    var serverUrl = window.location.origin;

    var projectName = getParameterByName('projectName');
    var analysisName = getParameterByName('analysisName');
    var analysisId = getParameterByName('analysisId');
    var datasetId = getParameterByName('datasetId');

    $(document).ready(function() {

        $('#nav').affix({
            offset: {
                top: $('header').height()
            }
        });

        $("[data-toggle=popover]").popover();

        $(".ctrl-asset-type-switcher").popover({
            html : true, 
            content: function() {
                return $('#content-asset-types').html();
            }
        });

        // put path in application navigator
        $('#nav-project').text(projectName);
        $('#nav-analysis').text(" \\ "+analysisName);

        loadModels();
    });
    
    function buildNotifications(message){
        if(message == null) {
	   	   return '';
    	}
        var builtErrors = 
        '<div class="ctrl-validation-panel-left col-md-12 col-centered">' +
        '<p>' + message + '</p>'
        '</div>';
        return builtErrors;
    }
    // function to build notifications
    function buildNotification(message, notificationType) {
        var builtNotification;
        if(notificationType == 'warning') {
            builtNotification = 
            '<div class="alert alert-warning alert-dismissible" role="alert">' +
                '<button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>' +
                '<img src="../../images/icons/ico-error.png" class="indi" />' +
                message +
            '</div>';            
        }
        else if(notificationType == 'info') {
            builtNotification = 
            '<div class="alert alert-info alert-dismissible" role="alert">' +
                '<button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>' +
                '<img src="../../images/icons/ico-alert.png" class="indi" />' +
                message +
            '</div>';            
        }
        else if(notificationType == 'success') {
            builtNotification = 
            '<div class="alert alert-success alert-dismissible" role="alert">' +
                '<button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>' +
                message +
            '</div>';            
        }
        return builtNotification;
    }    
    // function to handle notifications
    function handleNotification(notificationText, checkElement, closeElement, notificationType) {
        if(!$(checkElement).length) {
            $(closeElement).before(buildNotification(notificationText, notificationType));             
        }
    }    

    function loadModels() {
        
        $('#accordion').empty();

        $.ajax({
            type: "GET",
            url: serverUrl+"/api/analyses/"+analysisId+"/models",
            /*beforeSend : function(xhr) {
                xhr.setRequestHeader("Authorization", "Basic " + authEncoded);
            },*/
            success: function(data) {
                var analysisModels = data;
                
                if (analysisModels.length == 0) {
                    var accordionPanelNoModels = 
                    '<div class="ctrl-info-panel col-md-12 col-centered">'+
                        '<h2>You do not have any models created at the moment.</h2>'+
                        '<p><a href="create_project.jag" id="create-model" class="cu-level2-btn btn-add-new-dark" data-toggle="tooltip" data-original-title="Create Model"><span>Create Model</span></a></p>'+
                    '</div>';
                    $("#accordion").append(accordionPanelNoModels);    
                }
                else {

                    var createProject = '' + 
                    '<div class="ctrl-info-panel col-md-12 col-centered">'+
                        '<p><a href="create_project.jag" id="create-model" class="cu-level2-btn btn-add-new-dark" data-toggle="tooltip" data-original-title="Create Model"><span>Create Model</span></a></p>'+
                    '</div>';

                    $("#accordion").append(createProject);                        

                    for (var i = 0; i < analysisModels.length; i++) {

                        var modelStatus = analysisModels[i]['status'];

                        accordionPanel = '' + 
                            '<div class="panel panel-default">' +
                                '<div class="panel-heading" role="tab" id="heading'+i+'">' +
                                    '<h4 class="panel-title">' +
                                        '<table class="tbl-projects" projectID="' + analysisModels[i]['id'] + '">' +
                                            '<tr>' +
                                                '<td>' +
						    '<a data-toggle="collapse" data-parent="#accordion" href="#collapse'+i+'" aria-expanded="true" aria-' + 'controls="collapse1" class="ctrl-exp-col">' +
                                                    '<span class="cu-acc-head-title">' + analysisModels[i]['name'] + '</span>' +
                                                    '<span class="cu-acc-head-created">[ Created: ' + analysisModels[i]['createdTime'] + ' ]</span>' +
                                                    '<span class="cu-acc-head-workflow"></span>' +
                                                    '<span class="cu-acc-head-created">[ Status: ' + analysisModels[i]['status'] + ' ]</span>' +
                                                    '</a>' +
                                                '</td>' +
                                                '<td class="c-right">' +
                                                    '<div>' +
                                                        '<a href="#" id="view-model" class="cu-reg-btn btn-view '+appendClass(modelStatus)+'" data-modelname="'+analysisModels[i]['name']+'" data-modelid="'+analysisModels[i]['id']+'"><span>View</span></a>' +
                                                        '<a href="#" id="predict" class="cu-reg-btn btn-predict '+appendClass(modelStatus)+'" data-modelname="'+analysisModels[i]['name']+'" data-modelid="'+analysisModels[i]['id']+'"><span>Predict</span></a>' +
                                                        '<a href="'+serverUrl+'/api/models/'+analysisModels[i]['name']+'/import" id="download-model" class="cu-reg-btn btn-download '+appendClass(modelStatus)+'" data-modelname="'+analysisModels[i]['name']+'" data-modelid="'+analysisModels[i]['id']+'"><span>Download</span></a>' +
                                                        '<a href="#" id="publish" class="cu-reg-btn btn-publish '+appendClass(modelStatus)+'" data-modelid="'+analysisModels[i]['id']+'"><span>Publish</span></a>' +
                                                        '<a href="#" id="delete-model" class="cu-reg-btn btn-delete" data-modelid="'+analysisModels[i]['id']+'">Delete Model</a>' +
                                                    '</div>' +
                                                '</td>' +
                                            '</tr>' +
                                        '</table>' +
                                    '</h4>' +
                                '</div>' +
                                '<div id="collapse'+i+'" class="panel-collapse collapse" role="tabpanel" aria-labelledby="heading'+i+'">' +
                                    '<div class="panel-body">' +
					buildNotifications(analysisModels[i]['error']);
                                    '</div>' +
                                '</div>' +
                            '</div>';
                            $("#accordion").append(accordionPanel);
                    };
                }                
            },
            error: function() {
                var message = "An error occurred while retrieving tenant projects.";
            }
        });

        bindEvents();            
    }
    // function to append button disabled CSS class when model building is not complete
    function appendClass(modelStatus) {
        var classAppend = '';
        if(modelStatus != 'Complete') {
            classAppend = 'btn-disabled';  
        }
        return classAppend;
    }    

    function bindEvents() {

        $(document).on('click', '#btn-refresh', function(e){
            e.preventDefault();
            // reload from the server
            location.reload(true);
        });

        $(document).on('click', '#predict', function(e){ 
            e.preventDefault();
            
            var modelId = $(this).data('modelid');
            var modelName = $(this).data('modelname');
            window.location.href = '../predict/predict.jag?analysisId='+analysisId+'&datasetId='+datasetId+'&modelId='+modelId+'&projectName='+projectName+'&analysisName='+analysisName+'&modelName='+modelName;
        });

        $(document).on('click', '#publish', function(e){ 
            e.preventDefault();
            
            var modelId = $(this).data('modelid');

            $.ajax({
                type: "POST",
                url: serverUrl+"/api/models/"+modelId+"/publish",
                beforeSend : function(xhr) {
                    xhr.setRequestHeader("Authorization", "Basic " + authEncoded);
                },
                success : function(res){
                    var successText = 'Model successfully published to registry.';
                    handleNotification(successText, '.alert', '.ctrl-info-panel', 'success');
                },
                error : function(res){
                    if (crtValArea.prev("ctrl-validation-panel").length == 0){
                        crtValArea.before(buildNotifications(res.responseText));
                    }
                }
            });            
        });        

        $(document).on('click', '#view-model', function(e){ 
            e.preventDefault();
            
            var modelId = $(this).data('modelid');
            var modelName = $(this).data('modelname');
            window.location.href = 'view-model.jag?analysisId='+analysisId+'&datasetId='+datasetId+'&modelId='+modelId+'&projectName='+projectName+'&analysisName='+analysisName+'&modelName='+modelName;
        });        

        $(document).on('click', '#create-model', function(e){ 
            e.preventDefault();

            var algorithmName, algorithmType;

            $.ajax({
                type: "GET",
                url: serverUrl+"/api/analyses/"+analysisId+"/algorithmName",
                async : false,
                dataType: 'text',
                /*beforeSend : function(xhr) {
                    xhr.setRequestHeader("Authorization", "Basic " + authEncoded);
                },*/
                success : function(res){
                    algorithmName = res;
                },
                error : function(res){
                    if (crtValArea.prev("ctrl-validation-panel").length == 0){
                        crtValArea.before(buildNotifications(res.responseText));
                    }
                }
            });

            $.ajax({
                type: "GET",
                url: serverUrl+"/api/analyses/"+analysisId+"/algorithmType",
                async : false,
                dataType: 'text',
                /*beforeSend : function(xhr) {
                    xhr.setRequestHeader("Authorization", "Basic " + authEncoded);
                },*/
                success : function(res){
                    algorithmType = res;
                },
                error : function(res){
                    if (crtValArea.prev("ctrl-validation-panel").length == 0){
                        crtValArea.before(buildNotifications(res.responseText));
                    }
                }
            });

            window.location.href = '../model/model.jag?analysisId='+analysisId+'&datasetId='+datasetId+'&projectName='+projectName+'&analysisName='+analysisName+"&fromAnalysis=true"
                                    +'&algorithmName='+algorithmName+'&algorithmType='+algorithmType;
        });

        $(document).on('click', '#delete-model', function(e){ 
            e.preventDefault();
            var modelId = $(this).data('modelid');
            
            $.ajax({
                type: "DELETE",
                url: serverUrl+"/api/models/"+modelId,
                /*beforeSend : function(xhr) {
                    xhr.setRequestHeader("Authorization", "Basic " + authEncoded);
                },*/
                success : function(res){
                    $('#accordion').empty();
                    loadModels();
                },
                error : function(res){
                    //$('#workflow-name-error').text("* "+res.responseJSON.message);
                    if (crtValArea.prev("ctrl-validation-panel").length == 0){
                        crtValArea.before(buildNotifications(res.responseJSON.status));
                    }
                }
            });
        });

    }        

</script>

</body>

</html>
